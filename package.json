{
  "name": "release-clouddeploy",
  "version": "1.0.4",
  "description": "Github Action: Create a release to Google Cloud Deploy",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && ncc build",
    "lint": "eslint src/ --ext .ts,.tsx",
    "format": "prettier --write **/*.ts",
    "test": "mocha -r ts-node/register -t 150s 'tests/unit/*.test.ts' --exit",
    "e2e-tests": "mocha -r ts-node/register -t 150s 'tests/e2e.test.ts' --exit",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gcp-cloud-deploy-ecosystem/deploy-clouddeploy.git"
  },
  "keywords": [
    "actions",
    "google",
    "cloud deploy",
    "cloud",
    "deploy",
    "release",
    "skaffold",
    "containers"
  ],
  "author": "gcp-cloud-deploy-ecosystem",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gcp-cloud-deploy-ecosystem/deploy-clouddeploy/issues"
  },
  "homepage": "https://github.com/gcp-cloud-deploy-ecosystem/deploy-clouddeploy#readme",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/exec": "^1.1.1",
    "@actions/http-client": "^2.0.1",
    "@actions/tool-cache": "^2.0.1",
    "@lifeomic/attempt": "^3.0.3",
    "@types/tmp": "^0.2.3",
    "core-util-is": "^1.0.3",
    "fs": "0.0.2",
    "google-auth-library": "^8.5.2",
    "googleapis": "^108.0.0",
    "tmp": "^0.2.1",
    "typed-rest-client": "^1.8.9",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.7.23",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vercel/ncc": "^0.34.0",
    "chai": "^4.3.6",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lodash": "^4.17.21",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "sinon": "^14.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
